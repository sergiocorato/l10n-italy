<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
    <record id="view_tax_vsc_form" model="ir.ui.view">
        <field name="name">vsc.account.tax.code.form</field>
        <field name="model">account.tax.code</field>
        <field name="inherit_id"
               ref="account_vat_period_end_statement.view_tax_code_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat_statement_account_id']"
                   position="after">
                <field name="vsc_exclude_operation"/>
                <field name="vsc_exclude_vat"/>
            </xpath>
        </field>
    </record>
</data>
</openerp>
